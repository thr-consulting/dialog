{
  "name": "dialog",
  "version": "0.2.5",
  "description": "Dialog Addon that slides up from the bottom of the screen",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.babel.js",
    "demo": "react-scripts start",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "flow": "flow",
    "lint": "eslint *.js lib",
    "docs": "jsdoc -c jsdoc.json",
    "docs:explain": "jsdoc -X -c jsdoc.json",
    "validate": "npm ls"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thr-consulting/dialog.git"
  },
  "keywords": [
    "react",
    "controls",
    "dialog",
    "dock"
  ],
  "author": "Mike Kornelson <mike@durbn.net>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "flow-bin": "^0.49.1",
    "identity-obj-proxy": "^3.0.0",
    "ink-docstrap": "^1.3.0",
    "jest": "^20.0.4",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.3.0",
    "postcss-loader": "^2.0.6",
    "postcss-modules-values": "^1.3.0",
    "precommit-hook-eslint": "^3.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "semantic-ui-react": "^0.71.0",
    "style-loader": "^0.18.2",
    "webpack": "^2.2.0",
    "webpack-node-externals": "^1.6.0"
  },
  "peerDependencies": {
    "react": "^15.5.0",
    "semantic-ui-react": "^0.71.0"
  },
  "dependencies": {
    "debug": "^2.6.8",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react-dock": "^0.2.4",
    "react-flow-types": "^0.1.1",
    "tproptypes": "^0.1.11"
  },
  "pre-commit": [
    "lint",
    "flow",
    "test"
  ]
}
